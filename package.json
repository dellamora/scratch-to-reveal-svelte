{
  "name": "@dellamora/scratch-to-reveal-svelte",
  "version": "1.0.2",
  "description": "A simple and customizable scratch-to-reveal component for Svelte",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "keywords": [
    "svelte",
    "scratch",
    "reveal",
    "canvas",
    "interactive",
    "component"
  ],
  "author": "Francielle Dellamora",
  "repository": {
    "type": "git",
    "url": "https://github.com/dellamora/scratch-to-reveal-svelte"
  },
  "homepage": "https://github.com/dellamora/scratch-to-reveal-svelte#readme",
  "license": "MIT",
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.42.1",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/prismjs": "^1.26.5",
    "jsdom": "^27.0.0",
    "publint": "^0.1.9",
    "svelte": "^5.38.10",
    "svelte-check": "^4.3.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "type": "module",
  "dependencies": {
    "prismjs": "^1.30.0"
  }
}
